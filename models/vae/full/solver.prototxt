train_net: "./models/vae/full/train.prototxt"
base_lr: .0001
lr_policy: "step"
gamma: 0.1
stepsize: 20000
display: 10
average_loss: 100
# iter_size: 1
momentum: 0.9
weight_decay: 0.005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "vae_full_size_100"
iter_size: 2
