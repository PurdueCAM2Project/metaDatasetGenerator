train_net: "models/classification/imagenet/VGG16/train.prototxt"
base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
stepsize: 100000
display: 20
average_loss: 1
# iter_size: 1
momentum: 0.9
weight_decay: 0.005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "pascal_voc_al_net"
iter_size: 1

